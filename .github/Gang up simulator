Tab:AddButton({
    Name = "teleport to gun",
    Callback = function()
        -- Sauvegarder la position actuelle avant de se t√©l√©porter
local player = game.Players.LocalPlayer
local humanoidRootPart = player.Character.HumanoidRootPart
local originalPosition = humanoidRootPart.CFrame

-- Se t√©l√©porter √† une nouvelle position
humanoidRootPart.CFrame = CFrame.new(17.612613677978516, 2.999999761581421, -41.0633430480957)

-- Exemple pour se re-t√©l√©porter √† la position d'origine
wait(0.5)  -- Attendre 5 secondes avant de se re-t√©l√©porter

-- Re-t√©l√©portation √† la position d'origine
humanoidRootPart.CFrame = originalPosition
    end    
})

Tab:AddButton({
    Name = "Auto Farm",
    Callback = function()
        getgenv().lol = true -- set to false to disable
task.spawn(function()
        while true do wait()
            if not getgenv().lol then
                break
            end
            for i,v in pairs(workspace.Particles:WaitForChild("Money"):GetChildren()) do
                if v.Name == "Bundle" then
                    game:GetService("Players").LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame = v.CFrame
                end
            end
        end
end)
    end    
})

